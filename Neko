-- âš”ï¸ ğŸ†• è‡ªå‹•æ­¦è£è‰²å¾ªç’° (æ¯ç§’æª¢æŸ¥ä¸€æ¬¡)
-- =============================================================
task.spawn(function()
    while task.wait(1) do
        pcall(function()
            local character = localPlayer.Character
            if character then
                if not character:FindFirstChild("HasBuso") then
                    ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
                end
            end
        end)
    end
end)

