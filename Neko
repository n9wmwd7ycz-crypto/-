local function NuclearClean()
    local targets = {"NEKO BF", "NekoUI", "NekoTerminal_Lib", "NekoStickyMenu", "NekoMainBtn"}
    for _, n in ipairs(targets) do
        local o = CoreGui:FindFirstChild(n)
        if o then o:Destroy() end
    end
end
NuclearClean()
task.wait(0.2)

