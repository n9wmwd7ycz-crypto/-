-- æ ¸å¿ƒå¾ªç’°é‚è¼¯ï¼ˆå®Œæ•´ä¿ç•™ï¼‰
RunService.Heartbeat:Connect(function()
    local char = LP.Character
    if not char then return end
    local hum = char:FindFirstChild("Humanoid")
    if hum then
        if _G.CatSpeedActive then
            char:TranslateBy(hum.MoveDirection * (_G.CatSpeedValue / 10))
        end
        if _G.AutoV4 then
            pcall(function() char.Awakening.RemoteFunction:InvokeServer(true) end)
        end
        if _G.AutoBuso and not char:FindFirstChild("HasBuso") then
            ReplicatedStorage.Remotes.CommF_:InvokeServer("Buso")
        end
        if _G.EnergyValue and char:FindFirstChild("Energy") then
            char.Energy.Value = _G.EnergyValue
        end
    end
end)

RunService.Stepped:Connect(function()
    if _G.NoClip and LP.Character then
        for _, part in pairs(LP.Character:GetDescendants()) do
            if part:IsA("BasePart") then part.CanCollide = false end
        end
    end
end)

-- èˆªæµ·é‚è¼¯ï¼ˆå®Œæ•´ä¿ç•™ï¼‰
RunService.Heartbeat:Connect(function()
    local char = LP.Character
    if char and char:FindFirstChild("Humanoid") and char.Humanoid.SeatPart and char.Humanoid.SeatPart:IsA("VehicleSeat") then
        local seat = char.Humanoid.SeatPart
        local boat = seat.Parent
        if _G.BoatSpeedActive then seat.MaxSpeed = _G.BoatSpeedValue end
        -- å…¶ä»–èˆ¹é‚è¼¯ä¿æŒåŸæ¨£...
    end
end)

-- é€è¦– RenderSteppedï¼ˆå®Œæ•´ä¿ç•™ï¼‰
RunService.RenderStepped:Connect(function()
    -- ä½ çš„åŸé€è¦–é‚è¼¯...
end)

-- å¿«æ·éµ
UserInputService.InputBegan:Connect(function(input, processed)
    if processed then return end
    if input.KeyCode == Enum.KeyCode.G then
        _G.NoClip = not _G.NoClip
    elseif input.KeyCode == Enum.KeyCode.F then
        _G.BoatFlyActive = not _G.BoatFlyActive
    end
end)

-- é€šçŸ¥
