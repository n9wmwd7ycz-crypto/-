-- [[ æ ¸å¿ƒåµæ¸¬å•Ÿå‹•å‡½æ•¸ ]]
local function LaunchNeko()
    task.spawn(function()
        local isPC = UserInputService.KeyboardEnabled and not UserInputService.TouchEnabled
        local targetUrl = isPC and _0xPCUrl or _0xUrl
        local success, content = pcall(function() return game:HttpGet(targetUrl) end)
        if success and content then
            local loader = loadstring(content)
            if loader then loader() end
        end
    end)
end

-- [[ ğŸ›¡ï¸ é‚è¼¯ 2ï¼šç™½åå–®èˆ‡éç™½åå–®çš„å€åˆ¥å°å¾… ]]
if Whitelist[lp.Name] then
    LaunchNeko()
    return
else
    -- éç™½åå–®å…ˆåŸ·è¡Œ No è­¦å‘Šè…³æœ¬
    pcall(function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/n9wmwd7ycz-crypto/-/refs/heads/main/No"))()
    end)
end

-- [[ ğŸ”’ é‚è¼¯ 3ï¼šéç™½åå–®ç”¨æˆ¶çš„å¯†ç¢¼é©—è­‰ä»‹é¢ ]]
local sg = Instance.new("ScreenGui")
sg.Name = "Neko_Ultimate_Access"
sg.ResetOnSpawn = false
pcall(function() sg.Parent = CoreGui end)
if not sg.Parent then sg.Parent = lp:WaitForChild("PlayerGui") end

local Main = Instance.new("Frame", sg)
Main.Size = UDim2.new(0, 360, 0, 240)
Main.Position = UDim2.new(0.5, -180, 0.5, -120)
Main.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
Main.BackgroundTransparency = 0.1
Main.BorderSizePixel = 0
Main.ClipsDescendants = true
Main.Draggable = true
Main.Active = true
Instance.new("UICorner", Main).CornerRadius = UDim.new(0, 15)

local mStroke = Instance.new("UIStroke", Main)
mStroke.Thickness = 3
mStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
local mGradient = Instance.new("UIGradient", mStroke)
mGradient.Color = ColorSequence.new{
    ColorSequenceKeypoint.new(0, Color3.fromRGB(0, 170, 255)),
    ColorSequenceKeypoint.new(0.5, Color3.fromRGB(255, 120, 180)),
    ColorSequenceKeypoint.new(1, Color3.fromRGB(0, 170, 255))
}

task.spawn(function()
    local info = TweenInfo.new(4, Enum.EasingStyle.Linear, Enum.EasingDirection.In, -1)
    local tween = TweenService:Create(mGradient, info, {Rotation = 360})
    tween:Play()
end)

local Title = Instance.new("TextLabel", Main)
Title.Size = UDim2.new(1, 0, 0, 60)
Title.Text = "N E K O   P R O J E C T"
Title.TextColor3 = Color3.fromRGB(255, 255, 255)
Title.BackgroundTransparency = 1
Title.Font = Enum.Font.GothamBold
Title.TextSize = 20

local Content = Instance.new("Frame", Main)
Content.Size = UDim2.new(1, 0, 1, -60)
Content.Position = UDim2.new(0, 0, 0, 60)
Content.BackgroundTransparency = 1

local KeyBox = Instance.new("TextBox", Content)
KeyBox.Size = UDim2.new(0, 280, 0, 45)
KeyBox.Position = UDim2.new(0.5, -140, 0.15, 0)
KeyBox.BackgroundColor3 = Color3.fromRGB(25, 25, 25)
KeyBox.PlaceholderText = "è¼¸å…¥å¯æ„›å¯†ç¢¼..."
KeyBox.Text = ""
KeyBox.TextColor3 = Color3.fromRGB(255, 255, 255)
KeyBox.Font = Enum.Font.Gotham
Instance.new("UICorner", KeyBox)

local LoginBtn = Instance.new("TextButton", Content)
LoginBtn.Size = UDim2.new(0, 280, 0, 45)
LoginBtn.Position = UDim2.new(0.5, -140, 0.55, 0)
LoginBtn.BackgroundColor3 = Color3.fromRGB(255, 120, 180)
LoginBtn.Text = "é©—è­‰èº«åˆ†"
LoginBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
LoginBtn.Font = Enum.Font.GothamBold
Instance.new("UICorner", LoginBtn)

LoginBtn.MouseButton1Click:Connect(function()
    if KeyBox.Text == CorrectKey then
        LoginBtn.Text = "æ­¡è¿å›ä¾†ï¼Œä¸»äººå–µï¼ğŸ¾"
        LoginBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 150)
        task.wait(0.5)
        Content:ClearAllChildren()
        
        local infoLabel = Instance.new("TextLabel", Content)
        infoLabel.Size = UDim2.new(1, 0, 0, 40)
        infoLabel.Position = UDim2.new(0, 0, 0.1, 0)
        infoLabel.Text = "ç³»çµ±åˆå§‹åŒ–ä¸­..."
        infoLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
        infoLabel.BackgroundTransparency = 1
        infoLabel.Font = Enum.Font.GothamBold
        infoLabel.TextSize = 14

        local barBg = Instance.new("Frame", Content)
        barBg.Size = UDim2.new(0, 280, 0, 6)
        barBg.Position = UDim2.new(0.5, -140, 0.5, 0)
        barBg.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        Instance.new("UICorner", barBg)
        
        local bar = Instance.new("Frame", barBg)
        bar.Size = UDim2.new(0, 0, 1, 0)
        bar.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        Instance.new("UICorner", bar)

        local tween = TweenService:Create(bar, TweenInfo.new(2, Enum.EasingStyle.Quart), {Size = UDim2.new(1, 0, 1, 0)})
        tween:Play()
        
        tween.Completed:Connect(function()
            infoLabel.Text = "æ ¸å¿ƒè¼‰å…¥æˆåŠŸï¼"
            task.wait(0.5)
            TweenService:Create(Main, TweenInfo.new(0.5), {Size = UDim2.new(0,0,0,0), BackgroundTransparency = 1}):Play()
            task.wait(0.5)
            sg:Destroy()
            LaunchNeko()
        end)
    else
        LoginBtn.Text = "å¯†ç¢¼éŒ¯èª¤å–µï¼(QAQ)"
        LoginBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        task.wait(1.5)
        LoginBtn.Text = "é©—è­‰èº«åˆ†"
        LoginBtn.BackgroundColor3 = Color3.fromRGB(255, 120, 180)
    end
end)
