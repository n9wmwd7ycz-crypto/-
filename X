-- [[ 2. é©—è­‰èˆ‡è¼‰å…¥å‹•ç•«é‚è¼¯ ]]
LoginBtn.MouseButton1Click:Connect(function()
    if KeyBox.Text == CorrectKey then
        LoginBtn.Text = "æ­¡è¿å›ä¾†ï¼Œä¸»äººå–µï¼ğŸ¾"
        LoginBtn.BackgroundColor3 = Color3.fromRGB(0, 255, 150)
        task.wait(0.5)
        
        Content:ClearAllChildren()
        
        local info = Instance.new("TextLabel", Content)
        info.Size = UDim2.new(1, 0, 0, 40)
        info.Position = UDim2.new(0, 0, 0.1, 0)
        info.Text = "ç³»çµ±åˆå§‹åŒ–ä¸­..."
        info.TextColor3 = Color3.fromRGB(255, 255, 255)
        info.BackgroundTransparency = 1
        info.Font = Enum.Font.GothamBold
        info.TextSize = 14

        local barBg = Instance.new("Frame", Content)
        barBg.Size = UDim2.new(0, 280, 0, 6)
        barBg.Position = UDim2.new(0.5, -140, 0.5, 0)
        barBg.BackgroundColor3 = Color3.fromRGB(30, 30, 30)
        Instance.new("UICorner", barBg)
        
        local bar = Instance.new("Frame", barBg)
        bar.Size = UDim2.new(0, 0, 1, 0)
        bar.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
        Instance.new("UICorner", bar)

        local tween = TweenService:Create(bar, TweenInfo.new(2, Enum.EasingStyle.Quart), {Size = UDim2.new(1, 0, 1, 0)})
        tween:Play()
        
        tween.Completed:Connect(function()
            info.Text = "æ ¸å¿ƒè¼‰å…¥æˆåŠŸï¼"
            task.wait(0.5)
            TweenService:Create(Main, TweenInfo.new(0.5), {Size = UDim2.new(0,0,0,0), BackgroundTransparency = 1}):Play()
            task.wait(0.5)
            sg:Destroy()
            
            -- [[ 3. åŸ·è¡Œä¸»äººæŒ‡å®šçš„æœ€æ–°é€£çµ ]]
            task.spawn(function()
                local success, content = pcall(function() return game:HttpGet(_0xUrl) end)
                if success and content then
                    local loader = loadstring(content)
                    if loader then loader() end
                else
                    warn("ğŸ¾ NEKO: æ ¸å¿ƒè¼‰å…¥å¤±æ•—å–µï¼")
                end
                _0xUrl = nil
            end)
        end)
    else
        LoginBtn.Text = "å¯†ç¢¼éŒ¯èª¤å–µï¼(QAQ)"
        LoginBtn.BackgroundColor3 = Color3.fromRGB(255, 50, 50)
        task.wait(1.5)
        LoginBtn.Text = "é©—è­‰èº«åˆ†"
        LoginBtn.BackgroundColor3 = Color3.fromRGB(255, 120, 180)
    end
end)
